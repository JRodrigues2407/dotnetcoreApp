{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ServicePlanName":{
      "type": "string"
    },
    "WebAppName":{
      "type": "string"
    }
  },
  "variables": {
    "keyVaulTemplateUrl":"https://github.com/wesleycamargo/dotnetcoreApp/blob/feature/templatecomposto/dotnetcoreApp-Template/KeyVault.json"
  },
  "resources": [    
    {
      "type": "Microsoft.Resources/deployments",
      "name": "createWebapp",
      "apiVersion": "2018-05-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/wesleycamargo/dotnetcoreApp/feature/blanktemplate/dotnetcoreApp-Template/WebApp.json"
        },
        "parameters": {
          "ServicePlanName": {
            "value": "[parameters('ServicePlanName')]"
          },
          "WebAppName": {
            "value": "[parameters('WebAppName')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "name": "createKeyVault",
      "apiVersion": "2018-05-01",
      "dependsOn": [
        "createWebapp"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('keyVaulTemplateUrl')]"
        },
        "parameters": {
          "keyVaultName": {
            "value": "keyVaultArmTemplate"
          },
          "secretName": {
            "value": "secretName"
          },
          "secretValue":{
            "value":"secretValue"
          },
          "webAppName": {
            "value": "WebApp2nqoh7r5dqr7a4"
          }
        }
      }
    }
  ],
  "outputs": {}
}