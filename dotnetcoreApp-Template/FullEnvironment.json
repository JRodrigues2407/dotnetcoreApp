{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ServicePlanName":{
      "type": "string"
    },
    "WebAppName":{
      "type": "string"
    }
  },
  "variables": {},
  "resources": [{
    "type": "Microsoft.Resources/deployments",
    "name": "createwebapp",
    "apiVersion": "2018-05-01",
    "properties": {
      "mode": "Incremental",
      "templateLink": {
        "uri": "https://raw.githubusercontent.com/wesleycamargo/dotnetcoreApp/feature/blanktemplate/dotnetcoreApp-Template/WebApp.json"
      },
      "parameters": {
        "ServicePlanName": {
          "value": "[parameters('ServicePlanName')]"
        },
        "WebAppName": {
          "value": "[parameters('WebAppName')]"
        }
      }
    }
  }],
  "outputs": {
    "webAppName":{
      "type": "string",
      // "value":"[reference(concat(resourceId('Microsoft.Web/sites', parameters('WebAppName')), '/providers/Microsoft.ManagedIdentity/Identities/default'), '2015-08-31-PREVIEW').principalId]"
      //"value":"[reference(concat(resourceId('Microsoft.Web/sites', reference('linkedTemplate').outputs.greetingMessage.value), '/providers/Microsoft.ManagedIdentity/Identities/default'), '2015-08-31-PREVIEW').principalId]"
      "value": "[reference('createwebapp').outputs.WebAppName.value]"     
    },
    "webAppId":{
      "type": "string",
      "value":"[reference(concat(resourceId('Microsoft.Web/sites', reference('WebAppName')), '/providers/Microsoft.ManagedIdentity/Identities/default'), '2015-08-31-PREVIEW').principalId]"

    }
    
  }
}